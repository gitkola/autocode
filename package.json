{
  "name": "autocode",
  "version": "1.0.0",
  "description": "AI-assisted code development tool",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "webpack --config webpack.config.js && tsc",
