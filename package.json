  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.31",
    "concurrently": "^8.2.2",
    "electron": "^13.0.0",
    "electron-reloader": "^1.2.3",
    "ts-loader": "^9.5.1",
    "typescript": "^4.2.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
